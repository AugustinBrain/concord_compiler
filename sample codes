main(){
    unit Car { 
        string model; 
        int year; 
        decimal price;
    }
    
    unit Car myCar = {"Toyota", 2, 3};
    
    display("Car Model: " + myCar.model);
    display("Car Year: " + myCar.year);
    display("Car Price: " + myCar.price);
}


main(){
    int x;
    decimal y;
    letter z;
    string a;
    bool b;
    
    display("Enter int: ");    
    x = reads();
    
    display("Enter decimal: ");  
    y = reads();
    
    display("Enter letter: ");  
    z = reads();
    
    display("Enter string: ");
    a = reads();

    display("Enter bool: ");
    a = reads();
    
    display(x + " " + y + " " + z + " " + a + " " + b);
}

