main() {
    string s = "hello";
    i = 0;
    while (s[i] != '\0'){
        i++;
    }
}


main() {
    string str;
    int i = 0, length = 0, isPalindrome = 1;

    display("Enter a word: ");
    str = reads();


    while (str[length] != '\0') {
        length++;
    }
    
    while (i < (length / 2)) {
        length -=  i - 1;
        if (str[i] != str[length]) {
            isPalindrome = 0;
        }
        i++;
    }

    if (isPalindrome) {
        display("The word is a palindrome.");
    } else {
        display("The word is not a palindrome.");
    }
}